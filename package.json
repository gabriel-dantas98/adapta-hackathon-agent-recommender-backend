{
  "name": "adapta-hackathon-agent-recommender-backend",
  "version": "1.0.0",
  "description": "API para recomendação de produtos usando embeddings e similaridade",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "db:generate": "supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID --schema=public > src/types/database.ts"
  },
  "keywords": [
    "fastify",
    "langchain",
    "supabase",
    "embeddings",
    "recommendations"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.2.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@langchain/core": "^0.1.52",
    "@langchain/openai": "^0.0.25",
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.10.0",
    "dotenv": "^17.2.0",
    "fastify": "^4.26.2",
    "fastify-type-provider-zod": "^1.1.9",
    "langchain": "^0.1.30",
    "pgvector": "^0.1.8",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
